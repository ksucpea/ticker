bool foundIn(char arr[], String find) {
  int j = find.length() - 1;
  for (int i = 14; j >= 0; i--) {
    if (find.charAt(j) != arr[i]) {
      return false;
    }
    j--;
  }
  return true;
}

void scrollStocks() {
  startSec = millis();
  timer200 = millis();

  int scrollLength = 1;
  bool pause = false;
  int pauseSec = 0;
  int catPos = 0;
  int x1 = 0, x2 = marketOpen == 1 ? 24 : 36, y1 = 12, y2 = 20;
  bool right = true, down = true;
  while (scrollLength < (numSlides + 1) * 64) {
    display.setBrightness(60);
    second = (millis() - startSec) / 1000;

    if (second >= 60) {
      minute++;
      second = 0;
      startSec = millis();
    }
    if (minute >= 60) {
      minute = 0;
      hour++;
    }

    if (pause == false) {
      display.clearDisplay();
      scrollLength++;
    } else if (pause == true && second == pauseSec) {
      pause = false;
      scrollLength++;
    } else if (pause == true) {
    }

    for (int i = 0; i < numSlides; i++) {
      int xOffset = 64 - scrollLength + (i * 64);
      if (xOffset < -64 || xOffset > 64) continue;  // if not visible -> IGNORE!
      if (xOffset == 1 && pause == false) {         // if the slide reaches 1 -> NEED TO PAUSE!
        pause = true;
        pauseSec = second + pauseTime < 60 ? second + pauseTime : second + pauseTime - 60;
      }

      if (i >= numStocks) {  // NON-STOCK SLIDES

        int slideNum = i - numStocks;
        if (slideNum == 0) {
          if (pause) display.clearDisplay();
          int hour12 = hour > 12 ? hour - 12 : hour;
          display.setTextColor(c_red);
          display.setCursor(xOffset + 1, 1);
          display.setTextSize(2);
          display.print((hour12 < 10 ? "0" : "") + String(hour12));
          display.setTextSize(1);
          display.setTextColor(c_orange);
          display.setCursor(xOffset + 1, 16);
          display.print((second < 10 ? "0" : "") + String(second));
          display.setTextColor(c_yellow);
          display.setCursor(xOffset + 26, 1);
          display.print((minute < 10 ? "0" : "") + String(minute));
          display.setCursor(xOffset + 39, 1);
          display.print((hour >= 12 ? "PM" : "AM"));
          display.setTextSize(2);
          display.setTextColor(c_green);
          display.setCursor(xOffset + 26, 9);
          display.println(currentTemp);
          display.setTextSize(1);
          display.setTextColor(c_blue);
          display.setCursor(xOffset + (63 - (currentWeather.length() * 6)), 24);
          display.println(currentWeather);
        } else if (slideNum == 1) {
          if (millis() / 31 != timer200) {
            timer200 = millis() / 31;
            if (right == true) {
              x1++;
              x2++;
              if (x2 == 64) {
                right = false;
              }
            } else {
              x1--;
              x2--;
              if (x1 == 0) {
                right = true;
              }
            }
            if (down == true) {
              y1++;
              y2++;
              if (y2 == 32) {
                down = false;
              }
            } else {
              y1--;
              y2--;
              if (y1 == 0) {
                down = true;
              }
            }
