          if (pause == true) display.clearDisplay();
          display.setTextColor(marketOpen == 1 ? c_green : c_red);
          display.setCursor(xOffset + x1, y1);
          if (fake == false) display.print(marketOpen == 1 ? "OPEN" : "CLOSED");
