    String find2 = "seconds\":";
    while (client.available()) {

      for (int i = 1; i < 15; i++) {
        process[i - 1] = process[i];
      }
      process[14] = client.read();

      if (foundIn(process, find0)) {
        hour = client.readStringUntil(',').toInt();
      }
      if (foundIn(process, find1)) {
